server {
  listen 80;

  root /var/www/html;

  index index.html;

  server_name _;

  location / {
    try_files $uri $uri/ =404;
  }

  location /static {
        # handle static files directly, without forwarding to the application
        alias /home/ubuntu/my-things-client/static;
        expires 30d;
    }
}
